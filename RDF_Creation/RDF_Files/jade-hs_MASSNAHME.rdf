<rdf:RDF xmlns:jhs="http://www.jade-hs.de#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ontologie fuer das Masterprojekt: Triplestore-Datenbank Wintersemester 16/17</rdfs:comment>
        <owl:versionInfo>v1.0</owl:versionInfo>
        <owl:imports rdf:resource="http://www.example.org/foo" />
        <owl:priorVersion rdf:resource="http://ontoware.org/projects/swrc" />
    </owl:Ontology>



    <!-- PACKAGE: default -->
    <!-- Massnahme  -->
    <owl:Class rdf:about="Massnahme">
    </owl:Class>
    <!-- Foerderprogramm  -->
    <owl:Class rdf:about="Foerderprogramm">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="hatBedingung" />
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">
                    1
                </owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>


    <!-- PRAEDIKATE-->
    <!-- Foerderprogramm		unterstuetzt	Antragsteller -->
    <owl:ObjectProperty rdf:about="unterstuetzt">
        <rdfs:range rdf:resource="Antragsteller" />
        <rdfs:domain rdf:resource="Foerderprogramm" />
    </owl:ObjectProperty>
    <!-- Foerderprogramm		hat				Bedingung -->
    <owl:ObjectProperty rdf:about="hatBedingung">
        <rdfs:range rdf:resource="Bedingung" />
        <rdfs:domain rdf:resource="Foerderprogramm" />
    </owl:ObjectProperty>

    <!-- EIGENSCHAFTEN -->
    <!-- Foerderprogramm -->
    <owl:DatatypeProperty rdf:ID="IdentifikationsNummer">
        <rdfs:domain rdf:resource="Foerderprogramm" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger" />
    </owl:DatatypeProperty>



